{
  "name": "devops-lab-student-project",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint ."
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^7.0.0",
    "mongoose": "^8.21.0",
    "node-cron": "^3.0.3",
    "qrcode-terminal": "^0.12.0",
    "whatsapp-web.js": "^1.23.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.13",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "globals": "^16.5.0",
    "mongodb-memory-server": "^10.0.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.2.2",
    "vitest": "^4.0.15"
  },
  "vitest": {
    "coverage": {
      "provider": "v8",
      "all": true,
      "include": [
        "src/**/*.js"
      ],
      "lines": 80,
      "functions": 80,
      "branches": 70,
      "statements": 80
    }
  }
}
